            'house_position': '',
            'house_scope': '',
            'house_price': '',
            'house_type': '',
            'business_circle': '',
            'house_rentway': '',
            'house_area': '',
            'house_id': '',
            'housedel_id': '',
            'resblock_name': '',
        });
    </script>



