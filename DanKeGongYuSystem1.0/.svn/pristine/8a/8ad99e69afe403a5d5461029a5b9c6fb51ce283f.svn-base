        // 发送短信验证码方法
        function sendFn(phoneNum,picCode) {
            // 执行发送短信
            $.ajax({
                type: 'POST',
                url: "https://www.dankegongyu.com/collect/send-text-verify-code",
                data: {'mobile': phoneNum,'verify_code': picCode},
                async: false,
                error: function(msg) {
                    // ...
                },
                success: function (data) {
                    // 判断短信发送是否频繁
                    if(data.msg === '验证码发送频繁'){
                        $('.pic-code-wrap').show();

                        // 更新图形验证码
                        $('.pic-code-wrap a').click();

                        // 清空验证码输入框
                        $('.pic-code').val('');

                        globalObj.flag++;

                        return false;
                    }

                    // 判断短信是否发送成功
                    if (data['success']) {
                        // 显示60秒计时,获取短信按钮隐藏
						$(".get-note-btn").hide();
						$(".timer-area").show();

						var max = 60;

						var timer = null;

						// 启动计时器开始计时
						timer = setInterval(function () {
							if(max === 0) {
								clearInterval(timer);

                                $(".get-note-btn").show();
                                $(".timer-area").hide();

                                $(".timer-area span").text(60);
                                max = 60;

                                // 更新图形验证码
                                $('.pic-code-wrap a').click();

                                // 清空验证码输入框
                                $('.pic-code').val('');
							} else {
							    max--;
							    $(".timer-area span").text(max);
							}
                        },1000);
                    }
                }
            });
        }

        // 检查图形验证码是否正确方法
        function checkPicCode(phoneNum,picCode) {
            $.ajax({
                type: 'POST',
                url: "https://www.dankegongyu.com/collect/img-code",
                data: {'mobile': phoneNum, 'verify_code': picCode},
