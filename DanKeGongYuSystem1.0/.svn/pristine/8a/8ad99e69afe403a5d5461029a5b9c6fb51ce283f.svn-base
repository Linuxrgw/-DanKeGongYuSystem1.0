                    // ...
                },
                success: function (data) {
                    if (data.msg === '验证码不正确') {
                        $('.pic-code-wrap').show();

                        // 更新图形验证码
                        $('.pic-code-wrap a').click();

                        // 清空验证码输入框
                        $('.pic-code').val('');

                        globalObj.codeFlag = false;

                        // 提示显示
                        $(".pic-code-tip").show();
                    } else {
                        globalObj.codeFlag = true;
					}
                }
            });
        }

