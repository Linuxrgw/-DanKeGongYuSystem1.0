                        $('.pic-code-wrap a').click();

                        // 清空验证码输入框
                        $('.pic-code').val('');

                        globalObj.flag++;

                        return false;
                    }

                    // 判断短信是否发送成功
                    if (data['success']) {
                        // 显示60秒计时,获取短信按钮隐藏
						$(".get-note-btn").hide();
						$(".timer-area").show();

						var max = 60;

						var timer = null;

						// 启动计时器开始计时
						timer = setInterval(function () {
							if(max === 0) {
								clearInterval(timer);

                                $(".get-note-btn").show();
                                $(".timer-area").hide();

                                $(".timer-area span").text(60);
                                max = 60;

                                // 更新图形验证码
                                $('.pic-code-wrap a').click();

                                // 清空验证码输入框
                                $('.pic-code').val('');
							} else {
							    max--;
							    $(".timer-area span").text(max);
							}
                        },1000);
                    }
                }
            });
        }

        // 检查图形验证码是否正确方法
        function checkPicCode(phoneNum,picCode) {
            $.ajax({
                type: 'POST',
                url: "https://www.dankegongyu.com/collect/img-code",
                data: {'mobile': phoneNum, 'verify_code': picCode},
                async: false,
                error: function(msg) {
                    // ...
                },
                success: function (data) {
                    if (data.msg === '验证码不正确') {
                        $('.pic-code-wrap').show();

                        // 更新图形验证码
                        $('.pic-code-wrap a').click();

                        // 清空验证码输入框
                        $('.pic-code').val('');

                        globalObj.codeFlag = false;

                        // 提示显示
                        $(".pic-code-tip").show();
                    } else {
                        globalObj.codeFlag = true;
					}
                }
            });
        }

        // ajax提交数据方法
		function ajaxSubmit(data) {
            $.ajax({
                type: "POST",
                url: "https://www.dankegongyu.com/collect/ajax-info/bj",
                data: data,
                async: false,
                error: function (msg) {
                    // 失败提示显示
                    $(".submit-fail-tip").show().text("提交失败，请重试");
                },
                success: function (data) {
                    // 判断是否提交成功
                    if (!data.success) {
                        // 更新图形验证码
                        $('.pic-code-wrap a').click();

                        // 清空验证码输入框
                        $('.pic-code').val('');

                        // 失败提示显示
						$(".submit-fail-tip").show().text(data.msg);

                        return false;
                    } else {
                        // 弹出提示框
                        $(".entrust-success-tip ").show();
					}
                }
            });
        }

        // 点击关闭按钮，关闭委托成功弹出层
		$(".success-tip-off,.read-btn").on("click",function () {
            $(".success-tip-layer").hide();
            // 页面重载
            window.location.reload();
        });

        // 登录回显手机号码
		$(function () {
            // 登录回显手机号
            $.ajax({
                type: 'GET',
                url: '/web-api/user/info',
                async: false,
                error: function(msg) {
                    console.log(msg)
                },
                success: function(data) {
                    $('.phone-num').val(data.data.mobile)
                }
            });
        })
	</script>
