	<script>
        $(function () {

            reSize();
            $(window).resize(function () {
                reSize()
            });
            /* 轮播 */
            var i = 0;
            var timer = null;
            var onoff = true;
            var firstimg = $('.img li').first().clone();
            $('.img').append(firstimg).width(($('.img li').length + 1) * ($('.img li').width()));
            $(".total").text("/" + ($('.img li').length - 1));
            $('.next').click(function(){
                if(!onoff){return;}
                onoff = false;
                i++;
                $('.img').stop().animate({left : - i * 821}, 300, function(){
                    if(i == $('.img li').length - 1){
                        $('.img').css({left : 0});
                        i = 0;
                    }
                    onoff = true;
                });
                var num = i;
                num = i >= $('.img li').length - 1 ? 1 : (i+1)
                if (i==$('.img li').length - 1) {
                    $(".crt").text(num);
                }else{
                    $(".crt").text(num);
                }
                return false;
            });

            $('.prev').click(function(){
                i--;
                if (i == - 1) {
                    i=$('.img li').length - 2;
                    $('.img').css({left : - ($('.img li').length - 1) * 821});
                }
                $(".crt").text( i + 1);
                $('.img').stop().animate({left : - i * 821},300);
            });

            timer = setInterval(function(){
                i++;
                if (i == $('.img li').length - 1) {
                    i = 0;
                    $('.img').css({left : 0});
                }
                $('.img').stop().animate({left : - i * 821},300);
                var num = i;
                num = i >= $('.img li').length - 1 ? 1 : (i+1);
                if (i == $('.img li').length - 1) {
                    $(".crt").text(num)
                }else{
                    $(".crt").text(num)
                }
                return false;
            },3000);

            $('.reason_banner').hover(function(){
                clearInterval(timer);
            },function(){
                timer = setInterval(function(){
                    i++;
                    if (i == $('.img li').length) {
                        i = 1;
                        $('.img').css({left : 0});
                    }
                    $('.img').stop().animate({left : - i * 821},300);
                },3000)
            });

            $('.ques-list').each(function () {
                $(this).click(function () {
                    if (!$(this).hasClass('on')) {
                        $('.ques-list').removeClass('on');
                        $(this).addClass('on');
                        $('.list-hidden').slideUp(300);
                        $(this).find('.list-hidden').slideDown(300);
                    }else{
                        $(this).removeClass('on');
                        $(this).find('.list-hidden').slideUp(300);
                    }
                });
            })

            function reSize() {
                if (window.screen.width <= 750) {
                    console.log($(window).width())
                    window.location.href = 'https://www.dankegongyu.com/api-page/living-guide'
                }
            }
        })
	</script>


